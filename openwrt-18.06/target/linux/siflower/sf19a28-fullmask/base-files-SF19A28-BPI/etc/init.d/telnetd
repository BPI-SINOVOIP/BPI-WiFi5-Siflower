#!/bin/sh /etc/rc.common
START=99

USE_PROCD=1

PROG=telnetd

start_service () {
    procd_open_instance
    procd_set_param command "$PROG"

    procd_set_param respawn

    procd_close_instance
}

stop_service()
{
	kill `ps | grep telnetd | awk '{print $1}'`
}

shutdown()
{
	kill `ps | grep telnetd | awk '{print $1}'`
}

